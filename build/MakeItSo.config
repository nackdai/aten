<MakeItSo>
  <AllProjects>
    <CPPFolderPrefix prefix=""/>
    <RemoveLibraryPath path="../3rdparty/glfw/x64/src/Debug"/>
    <RemoveLibraryPath path="../3rdparty/glfw/x64/src/Release"/>
    <RemoveLibraryPath path="../3rdparty/glew/lib/Debug/x64"/>
    <RemoveLibraryPath path="../3rdparty/glew/lib/Release/x64"/>
    <RemoveConfiguration config="Debug|Win32"/>
    <RemoveConfiguration config="Profile|Win32"/>
    <RemoveConfiguration config="Profile|x64"/>
    <RemoveConfiguration config="Release|Win32"/>
    <RemoveLibrary library="glew32d"/>
    <RemoveLibrary library="glew32"/>
    <RemoveLibrary library="glfw3"/>
    <RemoveLibrary library="OpenGL32"/>
    <RemoveLibrary library="Shlwapi"/>
